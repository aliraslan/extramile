{
  "name": "extramile",
  "private": true,
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/graphql": "^14.0.7",
    "@types/node": "^11.9.4",
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "3.3.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.4.2",
    "argon2": "^0.20.1",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "graphql": "^14.1.1",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.10",
    "type-graphql": "^0.16.0",
    "typeorm": "0.2.13"
  },
  "scripts": {
    "postinstall": "yarn --cwd frontend install",
    "clean": "rimraf build node_modules frontend/build frontend/node_modules",
    "dev": "concurrently \"yarn start:server\" \"yarn start:frontend\" ",
    "start:server": "ts-node-dev --respawn src/index.ts",
    "start:frontend": "yarn --cwd frontend run start",
    "build:server": "tsc",
    "build:frontend": "yarn --cwd frontend run build --production",
    "prebuild": "rimraf build frontend/build",
    "build": "concurrently \"yarn build:server\" \"yarn build:frontend\" ",
    "postbuild": "mv frontend/build build/public"
  }
}
